{
  "name": "oneparking",
  "description": "OneParking Application",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./out/www.js",
    "test": "mocha ./tests/out/test.js",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src",
    "compiletests": "node ./node_modules/typescript/bin/tsc -p ./tests",
    "postinstall": "cd frontend/ && npm install --unsafe-perm",
    "buildWithFront": "cd frontend/ && npm run build && cd .. && gulp buildAll",
    "build": "gulp buildAll",
    "buildProduction": "cd frontend/ && npm run build && cd .. && gulp buildAllProd",
    "buildandrun": "gulp buildAll && npm start"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.3",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jsonwebtoken": "^7.3.0",
    "mongodb": "^2.2.25",
    "morgan": "~1.6.1",
    "redis": "^2.7.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^2.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "^2.8.1",
    "@types/crypto-js": "^3.1.33",
    "@types/debug": "^0.0.29",
    "@types/express": "^4.0.35",
    "@types/gulp": "^4.0.2",
    "@types/gulp-less": "^0.0.30",
    "@types/gulp-mocha": "^0.0.30",
    "@types/gulp-nodemon": "^0.0.30",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.2.14",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.10",
    "@types/power-assert": "^1.4.29",
    "@types/redis": "^2.6.0",
    "@types/serve-favicon": "^2.2.28",
    "@types/socket.io": "^1.4.29",
    "browser-sync": "^2.18.8",
    "gulp": "^3.9.1",
    "gulp-mocha": "^4.1.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-tsb": "^2.0.3",
    "marked": "^0.3.6",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.2.1"
  }
